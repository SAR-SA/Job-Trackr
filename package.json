{
  "name": "job-trackr",
  "version": "1.0.0",
  "description": "Job Trackr",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dylanbest15/Job-Trackr.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "express-session": "^1.17.1",
    "if-env": "^1.0.4",
    "mysql2": "^2.2.5",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "react-icons": "^3.11.0",
    "react-router-dom": "^5.2.0",
    "sequelize": "^6.3.5"
  },
  "bugs": {
    "url": "https://github.com/dylanbest15/Job-Trackr/issues"
  },
  "homepage": "https://github.com/dylanbest15/Job-Trackr#readme"
}
